<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>jt-manage</display-name>
  <error-page>
  	<error-code>500</error-code>
  	<location>/backend/500.jsp</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>backend/manage.jsp</welcome-file>
  </welcome-file-list>
 
  <listener>
    <listener-class>com.tedu.jt.listener.MyServletListener</listener-class>
  </listener>
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.tedu.jt.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>ProdAddServlet</display-name>
    <servlet-name>ProdAddServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.backend.ProdAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdAddServlet</servlet-name>
    <url-pattern>/ProdAddServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdListServlet</display-name>
    <servlet-name>ProdListServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.backend.ProdListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdListServlet</servlet-name>
    <url-pattern>/ProdListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdDelServlet</display-name>
    <servlet-name>ProdDelServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.backend.ProdDelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdDelServlet</servlet-name>
    <url-pattern>/ProdDelServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdInfoSevlet</display-name>
    <servlet-name>ProdInfoSevlet</servlet-name>
    <servlet-class>com.tedu.jt.web.backend.ProdInfoSevlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdInfoSevlet</servlet-name>
    <url-pattern>/ProdInfoSevlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProdUpdateServlet</display-name>
    <servlet-name>ProdUpdateServlet</servlet-name>
    <servlet-class>com.tedu.jt.web.backend.ProdUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProdUpdateServlet</servlet-name>
    <url-pattern>/ProdUpdateServlet</url-pattern>
  </servlet-mapping>
</web-app>